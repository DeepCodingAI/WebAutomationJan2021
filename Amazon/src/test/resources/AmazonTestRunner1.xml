    <parameter name="url" value="http://amazon.com"></parameter>

    <test name = "Amazon Home">

        <classes>
<!--            <class name="landingpage.PageTitle"></class>-->
<!--            <class name="landingpage.Search"></class>-->
            <class name="landingpage.TestSearch"></class>
        </classes>

    </test>

</suite>
