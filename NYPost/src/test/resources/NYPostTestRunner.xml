<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name ="NYPost Test Script" parallel="false">
    <parameter name="useCloudEnv" value="false"/>
    <parameter name="envName" value="browserstack"/>
    <parameter name="os" value="OS X"/>
    <parameter name="os_version" value="Sierra"/>
    <parameter name="browserName" value="chrome"/>
    <parameter name="browserVersion" value="80.0"/>
    <parameter name="url" value="https://nypost.com/"></parameter>

    <test name = "NYP Home">

        <classes>
            <class name="homepage.TestAllFunctionality"></class>
<!--            <class name="homepage.SectionArticleNews"></class>-->
        </classes>

    </test>

</suite>
